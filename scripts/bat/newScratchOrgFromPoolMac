# Hent scratch org fra pool
sfp pool:fetch --tag dev --targetdevhubusername $1 --alias $2 --setdefaultusername

# Dytter kildekoden til scratch org'en
sf project deploy start

# Tildeler tilatelsessett"
sf org assign permset --name HOT_Servicetjenesten_Admin

# Oppretter testdata
sf apex run --file scripts/apex/createTestData.apex

# Importerer testdata for STO
sf data import tree --plan dummy-data/sto/data/plan.json
sf data import tree --plan dummy-data/sto/plan.json 
# Importerer samtalenotater
sf data import tree --plan dummy-data/conversation_notes/plan.json 

# Oppretter testdata for STO 
sf apex run --file dummy-data/GenerateData.apex

